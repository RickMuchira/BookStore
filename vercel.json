{
    "version": 2,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "public/build" }
      },
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/build/(.*)",
        "headers": { "cache-control": "public,max-age=31536000,immutable" },
        "dest": "/build/$1"
      },
      {
        "src": "/(.*\\.(js|css|map|png|jpg|svg|woff2?))",
        "headers": { "cache-control": "public,max-age=31536000,immutable" },
        "dest": "/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/api/index.php"
      }
    ],
    "outputDirectory": "public"
  }
  