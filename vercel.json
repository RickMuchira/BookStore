{
    "version": 2,
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "public/build",
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/build/$1",
        "headers": {
          "cache-control": "public,max-age=31536000,immutable"
        }
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php"
      },
      {
        "src": "/.*",
        "dest": "/index.php"
      }
    ]
  }
  